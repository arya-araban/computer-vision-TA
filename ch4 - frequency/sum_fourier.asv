clear;

x = 127;y=127;
im1 = zeros(x,y);
distance = 10;
im1(ceil(x/2)+distance,ceil(y/2)-distance) = 255;
im1(ceil(x/2)-distance,ceil(y/2)+distance) = 255;


out = ifftshift(im1);
out = real(ifft2(out));

figure;
subplot(1,2,1);imshow(uint8(im1)); title('fourier')
subplot(1,2,2);imshow(out,[]); title('spatial')